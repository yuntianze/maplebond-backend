{
    "version": 2,
    "functions": {
        "backend/wsgi.py": {
          "maxDuration": 30,
          "type": "background"
        }
      },
    "builds": [
      {
        "src": "backend/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/wsgi.py"
      }
    ]
  }
  