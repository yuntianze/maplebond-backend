{
    "version": 2,
    "functions": {
        "base/*.py": {
          "maxDuration": 30
        },
        "src": "backend/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/wsgi.py"
      }
    ]
  }
  